# Database Configuration
DATABASE_URL="postgresql://ccwrapper:ccwrapper@localhost:5432/ccwrapper_dev?schema=public"

# JWT Configuration
JWT_SECRET="your-secret-key-min-32-characters-for-production"
JWT_EXPIRY="15m"
REFRESH_TOKEN_EXPIRY="7d"

# OAuth Configuration - Google
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
GOOGLE_REDIRECT_URI="http://localhost:3000/api/auth/oauth/google/callback"

# OAuth Configuration - GitHub
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"
GITHUB_REDIRECT_URI="http://localhost:3000/api/auth/oauth/github/callback"

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000
