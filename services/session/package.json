{
  "name": "@cc-wrapper/session-service",
  "version": "1.0.0",
  "description": "CC Wrapper Session Service - Workspace session persistence and recovery",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/app.ts --outdir=dist --target=bun",
    "start": "bun dist/index.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "bun run prisma/seed.ts"
  },
  "dependencies": {
    "@cc-wrapper/shared-types": "workspace:*",
    "@prisma/client": "^6.17.1",
    "elysia": "^1.4.12",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "prisma": "^6.17.1"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "keywords": [
    "session",
    "persistence",
    "workspace",
    "recovery",
    "cc-wrapper"
  ],
  "author": "CC Wrapper Team",
  "license": "MIT"
}